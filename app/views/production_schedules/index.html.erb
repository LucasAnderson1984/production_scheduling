<h1>Listing production_schedules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @production_schedules.each do |production_schedule| %>
      <tr>
        <td><%= link_to 'Show', production_schedule %></td>
        <td><%= link_to 'Edit', edit_production_schedule_path(production_schedule) %></td>
        <td><%= link_to 'Destroy', production_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Production schedule', new_production_schedule_path %>
